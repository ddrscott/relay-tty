{
  "name": "relay-tty",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "relay": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "react-router build && tsc -p tsconfig.node.json",
    "start": "NODE_ENV=production node server.js",
