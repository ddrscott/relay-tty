    "express": "^5.1.0",
    "compression": "^1.8.0",
    "morgan": "^1.10.0",
    "isbot": "^5.1.0",
    "node-pty": "^1.0.0",
    "ws": "^8.18.0",
    "commander": "^13.1.0",
    "cookie": "^1.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.12.0",
    "@types/express": "^5.0.0",
    "@types/ws": "^8.5.0",
    "@types/cookie": "^0.6.0",
    "@types/compression": "^1.7.0",
    "@types/morgan": "^1.9.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "@tailwindcss/vite": "^4.1.0",
    "tailwindcss": "^4.1.0",
    "daisyui": "^5.0.0",
    "vite-tsconfig-paths": "^4.3.0",
    "@xterm/xterm": "^5.5.0",
